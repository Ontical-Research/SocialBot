    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "supertest": "^7.2.2",
